
{
        "website":"youtube",
        
        "connectionSteps":[
        
            {
                "action":"goTo",
                "url":"https://accounts.google.com/ServiceLogin?sacu=1&continue=https%3A%2F%2Fwww.youtube.com%2Fsignin%3Faction_handle_signin%3Dtrue%26app%3Ddesktop%26feature%3Dsign_in_button%26hl%3Dfr%26next%3D%252F&hl=fr&service=youtube#identifier",
                "fail":{
                    "if":"redirected",
                    "noFailUrl":"https://accounts.google.com/ServiceLogin?sacu=1&continue=https%3A%2F%2Fwww.youtube.com%2Fsignin%3Faction_handle_signin%3Dtrue%26app%3Ddesktop%26feature%3Dsign_in_button%26hl%3Dfr%26next%3D%252F&hl=fr&service=youtube#identifier",
                    "actions":[],
                    "send":"alreadyConnected"
                }
            },
        
            {
                "action":"fill",
                "fields":[
                    {"name":"Email","value":"user"}
                ],
                "formAction":"https://accounts.google.com/signin/challenge/sl/password"
            },
        
            {"action":"click","buttons":["next"],"formAction":"https://accounts.google.com/signin/challenge/sl/password"},
        
            {
                "action":"fill",
                "fields":[
                    {"name":"Passwd","value":"pass"}
                ],
                "formAction":"https://accounts.google.com/signin/challenge/sl/password"
            },
        
            {"action":"click","buttons":["signIn"],"formAction":"https://accounts.google.com/signin/challenge/sl/password"},
        
            {
                "action":"nextPage",
                "fail":{
                    "if":"existingFields",
                    "fields":["Passwd"],
                    "formAction":"https://accounts.google.com/signin/challenge/sl/password",
                    "actions":[],
                    "send":"wrongPassword"
                }
            }
        
        ]
    }
